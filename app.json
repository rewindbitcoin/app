{
  "expo": {
    "name": "ThunderDen",
    "slug": "ThunderDen",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.landabaso.ThunderDen"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.landabaso.ThunderDen"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "./plugins/withIncreasedAndroidCursorWindowSize"
    ],
    "extra": {
      "eas": {
        "projectId": "83b28e9f-1d27-40ef-bac8-44da70b30f16"
      }
    }
  }
}
